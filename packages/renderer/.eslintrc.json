{
  "env": {
    "browser": true,
    "node": false
  },
  "extends": [],
  "parserOptions": {
    "parser": "@typescript-eslint/parser",
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    /** These rules are disabled because they are incompatible with prettier */
    "@typescript-eslint/ban-ts-comment": "off",
    "@typescript-eslint/no-explicit-any": "warn"
  }
}
